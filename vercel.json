{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [ 
      { "src": "/", "dest": "build/index.html" },
      { "src": "/user/(.*)", "dest": "index.js" },
      { "src": "/chat/(.*)", "dest": "index.js" },
      { "src": "/(.+)", "dest": "build/$1" }
    ]
  }
  